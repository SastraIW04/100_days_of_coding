package Day3;

public class Main {
    
    public static void main(String[] args) {
        
        boolean satu = 2>=1 ;
        boolean dua = 2<=1 ;
        boolean tiga = 2!=1 ;
        boolean empat = 2==1 ;
        
        System.out.println(satu);
        System.out.println(dua);
        System.out.println(tiga);
        System.out.println(empat);
    }
    
}
